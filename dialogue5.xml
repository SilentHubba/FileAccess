<?xml version="1.0" encoding="utf-8" ?>
<Dialogue xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="https://www.w3.org/2001/XMLSchema">
	<Nodes>
    		<DialogueNode>
			<NodeID>0</NodeID>
			<Name>none</Name>
			<Text>I wake up to the sound of the doorbell and check my clock. It’s already past noon?</Text>
			<Background>Bedroom</Background>
			<Options>
				<DialogueOption>
					<Text>Continue</Text>
					<DestinationNodeID>1</DestinationNodeID>
				</DialogueOption>
			</Options>
		</DialogueNode>
		<DialogueNode>
			<NodeID>1</NodeID>
			<Name>none</Name>
			<Text>The doorbell rings again. I’d better go answer it.</Text>
			<Options>
				<DialogueOption>
					<Text>Continue</Text>
					<DestinationNodeID>2</DestinationNodeID>
				</DialogueOption>
			</Options>
		</DialogueNode>
		<DialogueNode>
			<NodeID>2</NodeID>
			<Name>Sawyer</Name>
			<Text>Are you there, [main]? Sorry I missed your call earlier.</Text>
			<Background>Entryway</Background>
			<Options>
				<DialogueOption>
					<Text>Continue</Text>
					<DestinationNodeID>3</DestinationNodeID>
				</DialogueOption>
			</Options>
		</DialogueNode>
		<DialogueNode>
			<NodeID>3</NodeID>
			<Name>You</Name>
			<Text>Yeah, I’m coming…</Text>
			<Options>
				<DialogueOption>
					<Text>Continue</Text>
					<DestinationNodeID>4</DestinationNodeID>
				</DialogueOption>
			</Options>
		</DialogueNode>
		<DialogueNode>
			<NodeID>4</NodeID>
			<Name>none</Name>
			<Text>I gingerly open the door, scanning Sawyer’s face. Today, I can’t read their thoughts like I usually can.</Text>
			<Options>
				<DialogueOption>
					<Text>Continue</Text>
					<DestinationNodeID>5</DestinationNodeID>
				</DialogueOption>
			</Options>
		</DialogueNode>
		<DialogueNode>
			<NodeID>5</NodeID>
			<Name>Sawyer</Name>
			<Text>Lyanna told me what happened at school, so I slipped out during lunch. How are you feeling?</Text>
			<Sprite1>XXSawNeu</Sprite1>
			<Options>
				<DialogueOption>
					<Text>Continue</Text>
					<DestinationNodeID>6</DestinationNodeID>
				</DialogueOption>
			</Options>
		</DialogueNode>
		<DialogueNode>
			<NodeID>6</NodeID>
			<Name>You</Name>
			<Text>...</Text>
			<Sprite1>SawNeu</Sprite1>
			<Options>
				<DialogueOption>
					<Text>Continue</Text>
					<DestinationNodeID>7</DestinationNodeID>
				</DialogueOption>
			</Options>
		</DialogueNode>
		<DialogueNode>
			<NodeID>7</NodeID>
			<Name>Sawyer</Name>
			<Text>Can I come in?</Text>
			<Sprite1>XXSawNeu</Sprite1>
			<Options>
				<DialogueOption>
					<Text>Continue</Text>
					<DestinationNodeID>8</DestinationNodeID>
				</DialogueOption>
			</Options>
		</DialogueNode>
		<DialogueNode>
			<NodeID>8</NodeID>
			<Name>none</Name>
			<Text>I don’t say anything and Sawyer enters anyway.</Text>
			<Sprite1>SawNeu</Sprite1>
			<Options>
				<DialogueOption>
					<Text>Continue</Text>
					<DestinationNodeID>9</DestinationNodeID>
				</DialogueOption>
			</Options>
		</DialogueNode>
		<DialogueNode>
			<NodeID>9</NodeID>
			<Name>Sawyer</Name>
			<Text>Do you need to talk about what you’re going through? Don’t push yourself though.</Text>
			<Sprite1>XXSawNeu</Sprite1>
			<Options>
				<DialogueOption>
					<Text>Continue</Text>
					<DestinationNodeID>10</DestinationNodeID>
				</DialogueOption>
			</Options>
		</DialogueNode>
		<DialogueNode>
			<NodeID>10</NodeID>
			<Name>You</Name>
			<Text>It’s my fault.</Text>
			<Sprite1>SawNeu</Sprite1>
			<Options>
				<DialogueOption>
					<Text>Continue</Text>
					<DestinationNodeID>11</DestinationNodeID>
				</DialogueOption>
			</Options>
		</DialogueNode>
		<DialogueNode>
			<NodeID>11</NodeID>
			<Name>Sawyer</Name>
			<Text>Huh? What are you talking about?</Text>
			<Sprite1>XXSawNeu</Sprite1>
			<Options>
				<DialogueOption>
					<Text>Continue</Text>
					<DestinationNodeID>12</DestinationNodeID>
				</DialogueOption>
			</Options>
		</DialogueNode>
		<DialogueNode>
			<NodeID>12</NodeID>
			<Name>You</Name>
			<Text>My paraself protected mom, but I couldn’t.</Text>
			<Sprite1>SawNeu</Sprite1>
			<Options>
				<DialogueOption>
					<Text>Continue</Text>
					<DestinationNodeID>13</DestinationNodeID>
				</DialogueOption>
			</Options>
		</DialogueNode>
		<DialogueNode>
			<NodeID>13</NodeID>
			<Name>You</Name>
			<Text>It’s my fault that we crashed in the first place.</Text>
			<Sprite1>SawNeu</Sprite1>
			<Options>
				<DialogueOption>
					<Text>Continue</Text>
					<DestinationNodeID>14</DestinationNodeID>
				</DialogueOption>
			</Options>
		</DialogueNode>
		<DialogueNode>
			<NodeID>14</NodeID>
			<Name>You</Name>
			<Text>Is there something wrong with me, Sawyer?</Text>
			<Sprite1>SawNeu</Sprite1>
			<Options>
				<DialogueOption>
					<Text>Continue</Text>
					<DestinationNodeID>15</DestinationNodeID>
				</DialogueOption>
			</Options>
		</DialogueNode>
		<DialogueNode>
			<NodeID>15</NodeID>
			<Name>none</Name>
			<Text>My phone rudely interrupts us. I go to turn it off, but I see it’s from the hospital.</Text>
			<Sprite1>SawNeu</Sprite1>
			<Options>
				<DialogueOption>
					<Text>Continue</Text>
					<DestinationNodeID>16</DestinationNodeID>
				</DialogueOption>
			</Options>
		</DialogueNode>
		<DialogueNode>
			<NodeID>16</NodeID>
			<Name>none</Name>
			<Text>With trepidation, I pick up the phone.</Text>
			<Sprite1>SawNeu</Sprite1>
			<Options>
				<DialogueOption>
					<Text>Continue</Text>
					<DestinationNodeID>17</DestinationNodeID>
				</DialogueOption>
			</Options>
		</DialogueNode>
		<DialogueNode>
			<NodeID>17</NodeID>
			<Name>You</Name>
			<Text>Hello?</Text>
			<Sprite1>SawNeu</Sprite1>
			<Options>
				<DialogueOption>
					<Text>Continue</Text>
					<DestinationNodeID>18</DestinationNodeID>
				</DialogueOption>
			</Options>
		</DialogueNode>
		<DialogueNode>
			<NodeID>19</NodeID>
			<Name>Doctor</Name>
			<Text>This is [main], correct?</Text>
			<Sprite1>SawNeu</Sprite1>
			<Options>
				<DialogueOption>
					<Text>Continue</Text>
					<DestinationNodeID>20</DestinationNodeID>
				</DialogueOption>
			</Options>
		</DialogueNode>
		<DialogueNode>
			<NodeID>20</NodeID>
			<Name>You</Name>
			<Text>Yeah.</Text>
			<Sprite1>SawNeu</Sprite1>
			<Options>
				<DialogueOption>
					<Text>Continue</Text>
					<DestinationNodeID>21</DestinationNodeID>
				</DialogueOption>
			</Options>
		</DialogueNode>
		<DialogueNode>
			<NodeID>21</NodeID>
			<Name>Doctor</Name>
			<Text>I regret to inform you...</Text>
			<Sprite1>SawNeu</Sprite1>
			<Options>
				<DialogueOption>
					<Text>Continue</Text>
					<DestinationNodeID>22</DestinationNodeID>
				</DialogueOption>
			</Options>
		</DialogueNode>
		<DialogueNode>
			<NodeID>22</NodeID>
			<Name>none</Name>
			<Text>I don’t want to hear it. I hang up.</Text>
			<Sprite1>SawNeu</Sprite1>
			<Options>
				<DialogueOption>
					<Text>Continue</Text>
					<DestinationNodeID>23</DestinationNodeID>
				</DialogueOption>
			</Options>
		</DialogueNode>
		<DialogueNode>
			<NodeID>23</NodeID>
			<Name>none</Name>
			<Text>My phone clatters to the ground. Everything is blurry.</Text>
			<Sprite1>SawNeu</Sprite1>
			<Options>
				<DialogueOption>
					<Text>Continue</Text>
					<DestinationNodeID>24</DestinationNodeID>
				</DialogueOption>
			</Options>
		</DialogueNode>
		<DialogueNode>
			<NodeID>24</NodeID>
			<Name>Sawyer</Name>
			<Text>Are you alright? What happened?</Text>
			<Sprite1>XXSawNeu</Sprite1>
			<Options>
				<DialogueOption>
					<Text>Continue</Text>
					<DestinationNodeID>25</DestinationNodeID>
				</DialogueOption>
			</Options>
		</DialogueNode>
		<DialogueNode>
			<NodeID>25</NodeID>
			<Name>You</Name>
			<Text>...</Text>
			<Sprite1>SawNeu</Sprite1>
			<Options>
				<DialogueOption>
					<Text>Continue</Text>
					<DestinationNodeID>26</DestinationNodeID>
				</DialogueOption>
			</Options>
		</DialogueNode>
		<DialogueNode>
			<NodeID>26</NodeID>
			<Name>You</Name>
			<Text>She’s...gone.</Text>
			<Sprite1>SawNeu</Sprite1>
			<Options>
				<DialogueOption>
					<Text>Continue</Text>
					<DestinationNodeID>27</DestinationNodeID>
				</DialogueOption>
			</Options>
		</DialogueNode>
		<DialogueNode>
			<NodeID>27</NodeID>
			<Name>none</Name>
			<Text>My voice is barely audible.</Text>
			<Sprite1>SawNeu</Sprite1>
			<Options>
				<DialogueOption>
					<Text>Continue</Text>
					<DestinationNodeID>28</DestinationNodeID>
				</DialogueOption>
			</Options>
		</DialogueNode>
		<DialogueNode>
			<NodeID>28</NodeID>
			<Name>Sawyer</Name>
			<Text>Your mom?</Text>
			<Sprite1>XXSawNeu</Sprite1>
			<Options>
				<DialogueOption>
					<Text>Continue</Text>
					<DestinationNodeID>29</DestinationNodeID>
				</DialogueOption>
			</Options>
		</DialogueNode>
		<DialogueNode>
			<NodeID>29</NodeID>
			<Name>Sawyer</Name>
			<Text>I’m so sorry [main]. I-</Text>
			<Sprite1>XXSawNeu</Sprite1>
			<Options>
				<DialogueOption>
					<Text>Continue</Text>
					<DestinationNodeID>30</DestinationNodeID>
				</DialogueOption>
			</Options>
		</DialogueNode>
		<DialogueNode>
			<NodeID>30</NodeID>
			<Name>none</Name>
			<Text>We sit in silence for a while.</Text>
			<Sprite1>SawNeu</Sprite1>
			<Options>
				<DialogueOption>
					<Text>Continue</Text>
					<DestinationNodeID>31</DestinationNodeID>
				</DialogueOption>
			</Options>
		</DialogueNode>
		<DialogueNode>
			<NodeID>31</NodeID>
			<Name>none</Name>
			<Text>Almost mindlessly, I walk towards the stash of prisms my mom kept. I activate one without thinking.</Text>
			<Sprite1>SawNeu</Sprite1>
			<Options>
				<DialogueOption>
					<Text>Continue</Text>
					<DestinationNodeID>32</DestinationNodeID>
				</DialogueOption>
			</Options>
		</DialogueNode>
		<DialogueNode>
			<NodeID>32</NodeID>
			<Name>none</Name>
			<Text>The screen starts to activate.</Text>
			<Sprite1>SawNeu</Sprite1>
			<Options>
				<DialogueOption>
					<Text>Continue</Text>
					<DestinationNodeID>33</DestinationNodeID>
				</DialogueOption>
			</Options>
		</DialogueNode>
		<DialogueNode>
			<NodeID>33</NodeID>
			<Name>none</Name>
			<Text>Wait...</Text>
			<Sprite1>SawNeu</Sprite1>
			<Options>
				<DialogueOption>
					<Text>Continue</Text>
					<DestinationNodeID>34</DestinationNodeID>
				</DialogueOption>
			</Options>
		</DialogueNode>
		<DialogueNode>
			<NodeID>34</NodeID>
			<Name>none</Name>
			<Text>I shut down the prism just as it connects in terror.</Text>
			<Sprite1>SawNeu</Sprite1>
			<Options>
				<DialogueOption>
					<Text>Continue</Text>
					<DestinationNodeID>35</DestinationNodeID>
				</DialogueOption>
			</Options>
		</DialogueNode>
		<DialogueNode>
			<NodeID>35</NodeID>
			<Name>You</Name>
			<Text>Sawyer?</Text>
			<Sprite1>SawNeu</Sprite1>
			<Options>
				<DialogueOption>
					<Text>Continue</Text>
					<DestinationNodeID>36</DestinationNodeID>
				</DialogueOption>
			</Options>
		</DialogueNode>
		<DialogueNode>
			<NodeID>36</NodeID>
			<Name>none</Name>
			<Text>Sawyer responds, but weakly.</Text>
			<Sprite1>SawNeu</Sprite1>
			<Options>
				<DialogueOption>
					<Text>Continue</Text>
					<DestinationNodeID>37</DestinationNodeID>
				</DialogueOption>
			</Options>
		</DialogueNode>
		<DialogueNode>
			<NodeID>37</NodeID>
			<Name>You</Name>
			<Text>I just activated a prism. Oh god, did I just kill my mom again?</Text>
			<Sprite1>SawNeu</Sprite1>
			<Options>
				<DialogueOption>
					<Text>Continue</Text>
					<DestinationNodeID>38</DestinationNodeID>
				</DialogueOption>
			</Options>
		</DialogueNode>
		<DialogueNode>
			<NodeID>38</NodeID>
			<Name>Sawyer</Name>
			<Text>I- I don’t even know anymore.</Text>
			<Sprite1>XXSawNeu</Sprite1>
			<Options>
				<DialogueOption>
					<Text>Continue</Text>
					<DestinationNodeID>39</DestinationNodeID>
				</DialogueOption>
			</Options>
		</DialogueNode>
		<DialogueNode>
			<NodeID>39</NodeID>
			<Name>none</Name>
			<Text>I choke back a sob, then hear another one. I turn around. It seems like Sawyer was sniffling, but they seem to look composed once I sit back next to them.</Text>
			<Sprite1>SawNeu</Sprite1>
			<Options>
				<DialogueOption>
					<Text>Continue</Text>
					<DestinationNodeID>40</DestinationNodeID>
				</DialogueOption>
			</Options>
		</DialogueNode>
		<DialogueNode>
			<NodeID>40</NodeID>
			<Name>Sawyer</Name>
			<Text>Why don’t we take a walk? It’ll be nice to get some fresh air and exercise.</Text>
			<Sprite1>XXSawNeu</Sprite1>
			<Options>
				<DialogueOption>
					<Text>Continue</Text>
					<DestinationNodeID>41</DestinationNodeID>
				</DialogueOption>
			</Options>
		</DialogueNode>
		<DialogueNode>
			<NodeID>41</NodeID>
			<Name>none</Name>
			<Text>I silently follow Sawyer out the door.</Text>
			<Sprite1>SawNeu</Sprite1>
			<Options>
				<DialogueOption>
					<Text>Continue</Text>
					<DestinationNodeID>42</DestinationNodeID>
				</DialogueOption>
			</Options>
		</DialogueNode>
		<DialogueNode>
			<NodeID>42</NodeID>
			<Name>none</Name>
			<Text>I don’t even know where we're going. I just follow Sawyer.</Text>
			<Sprite1>SawNeu</Sprite1>
			<Background>ParkWalk</Background>
			<Options>
				<DialogueOption>
					<Text>Continue</Text>
					<DestinationNodeID>43</DestinationNodeID>
				</DialogueOption>
			</Options>
		</DialogueNode>
		<DialogueNode>
			<NodeID>43</NodeID>
			<Name>none</Name>
			<Text>As we walk, I try to sort through what’s on my mind.</Text>
			<Sprite1>SawNeu</Sprite1>
			<Options>
				<DialogueOption>
					<Text>Continue</Text>
					<DestinationNodeID>44</DestinationNodeID>
				</DialogueOption>
			</Options>
		</DialogueNode>
		<DialogueNode>
			<NodeID>44</NodeID>
			<Name>none</Name>
			<Text>What use are prisms? Why do they exist?</Text>
			<Sprite1>SawNeu</Sprite1>
			<Options>
				<DialogueOption>
					<Text>Continue</Text>
					<DestinationNodeID>45</DestinationNodeID>
				</DialogueOption>
			</Options>
		</DialogueNode>
		<DialogueNode>
			<NodeID>45</NodeID>
			<Name>none</Name>
			<Text>Don’t they just exist to cause pain?</Text>
			<Sprite1>SawNeu</Sprite1>
			<Options>
				<DialogueOption>
					<Text>Continue</Text>
					<DestinationNodeID>46</DestinationNodeID>
				</DialogueOption>
			</Options>
		</DialogueNode>
		<DialogueNode>
			<NodeID>46</NodeID>
			<Name>Sawyer</Name>
			<Text>All of these prisms, how much of my life is my own? As people create more and more parallel lives, this life means less and less.</Text>
			<Sprite1>XXSawNeu</Sprite1>
			<Options>
				<DialogueOption>
					<Text>Continue</Text>
					<DestinationNodeID>47</DestinationNodeID>
				</DialogueOption>
			</Options>
		</DialogueNode>
		<DialogueNode>
			<NodeID>47</NodeID>
			<Name>Sawyer</Name>
			<Text>Do you understand what I’m saying?</Text>
			<Sprite1>XXSawNeu</Sprite1>
			<Options>
				<DialogueOption>
					<Text>"Of course."</Text>
					<DestinationNodeID>48</DestinationNodeID>
				</DialogueOption>
				<DialogueOption>
					<Text>"Not really."</Text>
					<DestinationNodeID>51</DestinationNodeID>
				</DialogueOption>
			</Options>
		</DialogueNode>
		<DialogueNode>
			<NodeID>48</NodeID>
			<Name>Sawyer</Name>
			<Text>It’s depressing to think about.</Text>
			<Sprite1>XXSawNeu</Sprite1>
			<Options>
				<DialogueOption>
					<Text>Continue</Text>
					<DestinationNodeID>49</DestinationNodeID>
				</DialogueOption>
			</Options>
		</DialogueNode>
		<DialogueNode>
			<NodeID>49</NodeID>
			<Name>Sawyer</Name>
			<Text>I- I’m sorry for bringing it up so randomly, with everything you’re going through.</Text>
			<Sprite1>XXSawNeu</Sprite1>
			<Options>
				<DialogueOption>
					<Text>Continue</Text>
					<DestinationNodeID>50</DestinationNodeID>
				</DialogueOption>
			</Options>
		</DialogueNode>
		<DialogueNode>
			<NodeID>50</NodeID>
			<Name>none</Name>
			<Text>We stay quiet for a while.</Text>
			<Sprite1>SawNeu</Sprite1>
			<Options>
				<DialogueOption>
					<Text>Continue</Text>
					<!--converge-->
					<DestinationNodeID>54</DestinationNodeID>
				</DialogueOption>
			</Options>
		</DialogueNode>
		<DialogueNode>
			<NodeID>51</NodeID>
			<Name>Sawyer</Name>
			<Text>It’s like our place in the universe. We’re just a speck in the grand scheme of things, destined to meet a fiery fate...</Text>
			<Sprite1>XXSawNeu</Sprite1>
			<Options>
				<DialogueOption>
					<Text>Continue</Text>
					<DestinationNodeID>52</DestinationNodeID>
				</DialogueOption>
			</Options>
		</DialogueNode>
		<DialogueNode>
			<NodeID>52</NodeID>
			<Name>Sawyer</Name>
			<Text>There’s so many versions of me who do something else that my actions don’t mean anything anymore.</Text>
			<Sprite1>XXSawNeu</Sprite1>
			<Options>
				<DialogueOption>
					<Text>Continue</Text>
					<DestinationNodeID>53</DestinationNodeID>
				</DialogueOption>
			</Options>
		</DialogueNode>
		<DialogueNode>
			<NodeID>53</NodeID>
			<Name>none</Name>
			<Text>I don’t respond.</Text>
			<Sprite1>SawNeu</Sprite1>
			<Options>
				<DialogueOption>
					<Text>Continue</Text>
					<DestinationNodeID>50</DestinationNodeID>
				</DialogueOption>
			</Options>
		</DialogueNode>
		<DialogueNode>
			<NodeID>54</NodeID>
			<Name>none</Name>
			<Text>Sawyer and I eventually make our way to the bridge near the edge of town. I look down at the water hundreds of meters below us.</Text>
			<Sprite1>SawNeu</Sprite1>
			<Background>Bridge</Background>
			<Options>
				<DialogueOption>
					<Text>Continue</Text>
					<DestinationNodeID>55</DestinationNodeID>
				</DialogueOption>
			</Options>
		</DialogueNode>
		<DialogueNode>
			<NodeID>55</NodeID>
			<Name>none</Name>
			<Text>It’s oddly beautiful.</Text>
			<Sprite1>SawNeu</Sprite1>
			<Options>
				<DialogueOption>
					<Text>Continue</Text>
					<DestinationNodeID>56</DestinationNodeID>
				</DialogueOption>
			</Options>
		</DialogueNode>
		<DialogueNode>
			<NodeID>56</NodeID>
			<Name>Sawyer</Name>
			<Text>[main], I know this is tough for you...</Text>
			<Sprite1>XXSawNeu</Sprite1>
			<Options>
				<DialogueOption>
					<Text>Continue</Text>
					<DestinationNodeID>57</DestinationNodeID>
				</DialogueOption>
			</Options>
		</DialogueNode>
		<DialogueNode>
			<NodeID>57</NodeID>
			<Name>Sawyer</Name>
			<Text>But your mom was a great person, right?</Text>
			<Sprite1>XXSawNeu</Sprite1>
			<Options>
				<DialogueOption>
					<Text>Continue</Text>
