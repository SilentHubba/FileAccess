					<Text>Continue</Text>
					<DestinationNodeID></DestinationNodeID>
				</DialogueOption>
			</Options>
		</DialogueNode>
		<DialogueNode>
			<NodeID></NodeID>
			<Name></Name>
			<Text></Text>
